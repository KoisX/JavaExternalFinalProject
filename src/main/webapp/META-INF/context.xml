<?xml version="1.0" encoding="UTF-8" ?>
<Context antiJARLocking="true" path="/">
    <Resource
        name="jdbc/student_testing?serverTimezone=UTC"
        author="Container"
        type="javax.sql.DataSource"
        maxActive="32"
        maxIdle="8"
        maxWait="10000"
        username="root"
        password="root"
        url="jdbc:mysql://localhost:3306/student_testing_db"
        driverClassName="com.mysql.jdbc.Driver"
    />
</Context>